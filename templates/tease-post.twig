{% extends "tease.twig" %}

{% block content %}
	<div class="container-900">
		<div class="row">
			<div class="col-md-12">
				<div class="bg-white rounded p-all-15 shadow--effect">
					<a href="{{post.link}}" class="d-block">
					<h2 class="h2">{{post.title}}</h2>
					<p class="text-muted">{{post.preview.length(25)}}</p>

					</a>
				</div>
			</div>
		</div>
	</div>
{% endblock %}